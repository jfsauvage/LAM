	Doi = {10.1364/AO.54.005281},
	Journal = {Appl. Opt.},
	Keywords = {Probability theory, stochastic processes, and statistics; Active or adaptive optics; Tomography; Imaging through turbulent media},
	Month = {Jun},
	Number = {17},
	Pages = {5281--5290},
	Publisher = {OSA},
	Title = {Spatio-angular minimum-variance tomographic controller for multi-object adaptive-optics systems},
	Url = {http://ao.osa.org/abstract.cfm?URI=ao-54-17-5281},
