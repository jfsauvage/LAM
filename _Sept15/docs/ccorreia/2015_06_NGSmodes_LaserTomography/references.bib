	Keywords = {Image reconstruction-restoration},
	Month = {May},
	Number = {5},
	Pages = {1016--1025},
	Publisher = {OSA},
	Title = {Maximum a posteriori estimation of fixed aberrations, dynamic aberrations, and the object from phase-diverse speckle data},
	Url = {http://josaa.osa.org/abstract.cfm?URI=josaa-16-5-1016},
	Volume = {16},
	Year = {1999},
