@article{davidoiu11,
	Abstract = {Several methods of phase retrieval for in-line phase tomography have
	already been investigated based on the linearization of the relation
	between the phase shift induced by the object and the diffracted
	intensity. In this work, we present a non-linear iterative approach
	using the Frechet derivative of the intensity recorded at a few number
	of propagation distances. A Landweber type iterative method with
	an analytic calculation of the Frechet derivative adjoint is proposed.
	The inverse problem is regularized with the smoothing L2 norm of
	the phase gradient and evaluated for several different implementations.
	The evaluation of the method was performed using a simple phase map,
	both with and without noise. Our approach outperforms the linear
	methods on simulated noisy data up to high noise levels and thanks
	to the proposed analytical calculation is suited to the processing
	of large experimental image data sets.},
	Author = {Davidoiu, V. and Sixou, B. and Langer, M. and Peyrin, F.},
	Date-Added = {2013-01-26 10:05:56 -0800},
	Date-Modified = {2013-01-26 10:06:01 -0800},
	Doi = {10.1364/OE.19.022809},
