@article{cortes12,
	Abstract = {This paper describes the data pre-processing and reduction methods
	together with SLOpe Detection And Ranging (SLODAR) analysis and wind
	profiling techniques for the Gemini South Multi-Conjugate Adaptive
	Optics System (GeMS).The wavefront gradient measurements of the five
	GeMS Shack--Hartmann sensors, each pointing to a laser guide star,
	are combined with the deformable mirror (DM) commands sent to three
	DMs optically conjugated at 0, 4.5 and 9 km in order to reconstruct
	pseudo-open loop slopes.These pseudo-open loop slopes are then used
	to reconstruct atmospheric turbulence profiles, based on the SLODAR
	and wind-profiling methods. We introduce the SLODAR method, and how
	it has been adapted to work in a closed-loop, multi-laser guide star
	system. We show that our method allows characterizing the turbulence
	of up to 16 layers for altitudes spanning from 0 to 19 km. The data
	pre-processing and reduction methods are described, and results obtained
	from observations made in 2011 are presented. The wind profiling
	analysis is shown to be a powerful technique not only for characterizing
	the turbulence intensity, wind direction and speed, but also as it
	can provide a verification tool for SLODAR results. Finally, problems
	such as the fratricide effect in multiple laser systems due to Rayleigh
	scattering, centroid gain variations, and limitations of the method
	are also addressed.},
	Author = {Cort{\'e}s, Angela and Neichel, Benoit and Guesalaga, Andr{\'e}s and Osborn, James and Rigaut, Francois and Guzman, Dani},
	Date-Added = {2012-11-21 09:57:06 -0800},
	Date-Modified = {2012-11-21 09:57:13 -0800},
	Doi = {10.1111/j.1365-2966.2012.22076.x},
	Issn = {1365-2966},
	Journal = {Monthly Notices of the Royal Astronomical Society},
	Keywords = {atmospheric effects, instrumentation: adaptive optics, methods: data analysis, site testing},
	Number = {3},
	Pages = {2089--2099},
	Title = {Atmospheric turbulence profiling using multiple laser star wavefront sensors},
	Url = {http://dx.doi.org/10.1111/j.1365-2966.2012.22076.x},
	Volume = {427},
	Year = {2012},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2012.22076.x}}

