	Keywords = {Wave-front sensing; Inverse problems; Active or adaptive optics},
	Month = {May},
	Number = {5},
	Pages = {1046--1059},
	Publisher = {OSA},
	Title = {Fast minimum variance wavefront reconstruction for extremely large telescopes},
